{
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 88,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-css-order",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        },
        {
            "files": "*.css",
            "options": {
                "cssDeclarationSorterOrder": "concentric-css"
            }
        }
    ]
}
